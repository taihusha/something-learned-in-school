.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

Page{
  background-color: #161616;
  padding-bottom: 116rpx;
  overflow: auto;
  box-sizing: border-box;
}

.navibar{
  width: 100%;
  height: 150rpx;
  position: fixed;
  z-index: 1;
  background-color: black;
}

.background{
  width: 100%;
  height: 1333rpx;
  z-index: 2;
}

.bg_panel{
  width: 100%;
  height: 150rpx;
  position: fixed;
  z-index: 3;
}

.header {
  width: 655rpx;
  padding: 30rpx 50rpx;
  line-height: 60rpx;
  color: #aaa;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

.indent {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.container {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.false{
  width: 600rpx;
  height: fit-content;
  margin-top: 50rpx;
  display: flex;
  justify-content: left;
  background-color: #323232;
  border-radius: 10rpx 10rpx 10rpx 10rpx / 10rpx 10rpx 10rpx 10rpx;
  border: 10rpx solid #161616;
}

.false::after {
  border: none;
}

.true {
  width: 600rpx;
  height: fit-content;
  margin-top: 50rpx;
  display: flex;
  justify-content: left;
  background-color: #323232;
  border-radius: 10rpx 10rpx 10rpx 10rpx / 10rpx 10rpx 10rpx 10rpx;
  border: 10rpx solid #2dabff;
}

.list {
  width: 600rpx;
  height: fit-content;
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #323232;
  border: 10rpx solid #161616;
}

.list::after {
  border: none;
}

.list_tapped {
  width: 600rpx;
  height: fit-content;
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #323232;
  border: 10rpx solid #2dabff;
}

.list_text {
  width: 100%;
  margin-top: -50rpx;
  margin-left: 10rpx;
  margin-bottom: 12rpx;
  text-align: left;
  line-height: 60rpx;
  color: #aaa;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

.label_text {
  max-width: 90%;
  margin-top: -50rpx;
  margin-left: 30rpx;
  margin-bottom: 12rpx;
  text-align: left;
  line-height: 60rpx;
  color: #aaa;
  font-size: 35rpx;
  letter-spacing: 3rpx;
}

.list.error {
  padding-left: 94rpx;
  color: #f66;
}

.list.success {
  padding-left: 94rpx;
  color: #6c0;
}

.list.error .number, .list.success .number {
  display: none;
}

.list.active {
  background: #fbfbfb;
}

.list.active .number {
  background: #eee;
  color: #777;
}

.list.active-success {
  background: #fbfbfb;
  color: #6c0;
}

.list.active-success .number {
  background: #6c0;
  color: #fff;
}

.footer-layer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.footer {
  position: fixed;
  box-sizing: border-box;
  bottom: 0;
  width: 100%;
  transform: translate3d(0, 100%, 0px);
  z-index: 100;
}

.count {
  position: absolute;
  box-sizing: border-box;
  top: -100rpx;
  height: 100rpx;
  width: 100%;
  padding: 35rpx 30rpx;
  font-size: 24rpx;
  overflow: hidden;
  zoom: 1;
  z-index: 1000;
  /*background: white;*/
  background: black;
}

.count view {
  float: right;
  line-height: 34rpx;
}

.count .collect {
  float: left;
  height: 34rpx;
  padding-left: 45rpx;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  font-size: 30rpx;
  letter-spacing: 2rpx;
  color: #aaa;
  line-height: 34rpx;
}

.count .collect.active {
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
}

.count .page {
  height: 30rpx;
  padding: 0 0 0 37rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/28rpx 28rpx;
  line-height: 36rpx;
}

.count .error {
  height: 30rpx;
  margin-right: 30rpx;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center/30rpx 30rpx;
  font-size: 24rpx;
  color: #aaa;
  vertical-align: middle;
  line-height: 36rpx;
}

.count .success {
  height: 30rpx;
  margin-right: 30rpx;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center/30rpx 30rpx;
  font-size: 24rpx;
  color: #6c0;
  vertical-align: middle;
  line-height: 36rpx;
}

.submit {
  float: right;
  width: 200rpx;
  height: 70rpx;
  margin: 25rpx 55rpx 0 20rpx;
  border-radius: 10rpx 10rpx 10rpx 10rpx / 10rpx 10rpx 10rpx 10rpx;
  background-color:#484848;
  font-size: 35rpx;
  line-height: 70rpx;
  text-align: center;
  color: #eeeeee;
  letter-spacing: 5rpx;
}

.submit_tapped {
  float: right;
  width: 200rpx;
  height: 70rpx;
  margin: 25rpx 55rpx 0 20rpx;
  border-radius: 10rpx 10rpx 10rpx 10rpx / 10rpx 10rpx 10rpx 10rpx;
  background-color:#323232;
  font-size: 35rpx;
  line-height: 70rpx;
  text-align: center;
  color: #aaa;
  letter-spacing: 5rpx;
}

.status-lists {
  max-height: 500rpx;
  padding: 0 0 25rpx 15rpx;
  background: #161616;
  z-index: 100;
  overflow: auto;
}

.status-lists .status-list {
  float: left;
  width: 70rpx;
  height: 70rpx;
  margin: 25rpx 0 0 20rpx;
  border-radius: 50%;
  background-color:#484848;
  font-size: 35rpx;
  line-height: 70rpx;
  text-align: center;
  color: #161616;
}

.status-lists .status-list.active {
  background: #eeeeee;
}

.status-lists .status-list.success {
  color: #eeeeee;
  background: #2dabff;
}

.status-lists .status-list.success.active {
  color: #2dabff;
  background: #eeeeee;
}

/**swiper**/

page {
  height: 100%;
}

.space {
  height: 20rpx;
}

.swiper-lists {
  position: relative;
  width: 100%;
  height: 1085rpx;
}

.swiper-lists .swiper-list {
  position: absolute;
  width: 100%;
  height: 1085rpx;
  background: #161616;
}

.swiper-lists .swiper-list.prev {
  transform: translate3d(-100%, 0, 0px);
}

.swiper-lists .swiper-list.next {
  transform: translate3d(100%, 0, 0px);
}

